<% @title = 'New Season' %>
<h1>New season</h1>

<% form_for(@season) do |f| %>
    <%= f.error_messages %>
    <p>
        <label for="season_name">Name</label><br/>
        <%= text_field 'season', 'name', :id=> 'season_name' %>
    </p>
    <p>
        <%= f.submit "Create", :id=>'submit' %>
    </p>
<% end %>

<%= link_to 'Back', seasons_path, :id=>'back' %>
